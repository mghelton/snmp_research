luxlPortStatusVeriPhyResultEntry OBJECT-TYPE
    SYNTAX      LUXLPortStatusVeriPhyResultEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each port has a set of VeriPhy results"
    INDEX       { luxlPortStatusVeriPhyResultIfIndex }
    ::= { luxlPortStatusVeriPhyResultTable 1 }

LUXLPortStatusVeriPhyResultEntry ::= SEQUENCE {
    luxlPortStatusVeriPhyResultIfIndex      LUXLInterfaceIndex,
    luxlPortStatusVeriPhyResultVeriPhyStatusPairA   LUXLPortPhyVeriPhyStatus,
    luxlPortStatusVeriPhyResultVeriPhyStatusPairB   LUXLPortPhyVeriPhyStatus,
    luxlPortStatusVeriPhyResultVeriPhyStatusPairC   LUXLPortPhyVeriPhyStatus,
    luxlPortStatusVeriPhyResultVeriPhyStatusPairD   LUXLPortPhyVeriPhyStatus,
    luxlPortStatusVeriPhyResultVeriPhyLengthStatusPairA     LUXLUnsigned8,
    luxlPortStatusVeriPhyResultVeriPhyLengthStatusPairB     LUXLUnsigned8,
    luxlPortStatusVeriPhyResultVeriPhyLengthStatusPairC     LUXLUnsigned8,
    luxlPortStatusVeriPhyResultVeriPhyLengthStatusPairD     LUXLUnsigned8
}

luxlPortStatusVeriPhyResultIfIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Logical interface number."
    ::= { luxlPortStatusVeriPhyResultEntry 1 }

luxlPortStatusVeriPhyResultVeriPhyStatusPairA OBJECT-TYPE
    SYNTAX      LUXLPortPhyVeriPhyStatus
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "VeriPhy status for the cable pair
         
         0 - Cable is Correctly terminated pair
         
         1 - Open pair
         
         2 - Shorted pair
         
         4 - Abnormal termination
         
         8 - Cross-pair short to pair A
         
         9 - Cross-pair short to pair B
         
         10 - Cross-pair short to pair C
         
         11 - Cross-pair short to pair D
         
         12 - Abnormal cross-pair coupling - pair A
         
         13 - Abnormal cross-pair coupling - pair B
         
         14 - Abnormal cross-pair coupling - pair C
         
         15 - Abnormal cross-pair coupling - pair D"
    ::= { luxlPortStatusVeriPhyResultEntry 2 }

luxlPortStatusVeriPhyResultVeriPhyStatusPairB OBJECT-TYPE
    SYNTAX      LUXLPortPhyVeriPhyStatus
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "VeriPhy status for the cable pair
         
         0 - Cable is Correctly terminated pair
         
         1 - Open pair
         
         2 - Shorted pair
         
         4 - Abnormal termination
         
         8 - Cross-pair short to pair A
         
         9 - Cross-pair short to pair B
         
         10 - Cross-pair short to pair C
         
         11 - Cross-pair short to pair D
         
         12 - Abnormal cross-pair coupling - pair A
         
         13 - Abnormal cross-pair coupling - pair B
         
         14 - Abnormal cross-pair coupling - pair C
         
         15 - Abnormal cross-pair coupling - pair D"
    ::= { luxlPortStatusVeriPhyResultEntry 3 }

luxlPortStatusVeriPhyResultVeriPhyStatusPairC OBJECT-TYPE
    SYNTAX      LUXLPortPhyVeriPhyStatus
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "VeriPhy status for the cable pair
         
         0 - Cable is Correctly terminated pair
         
         1 - Open pair
         
         2 - Shorted pair
         
         4 - Abnormal termination
         
         8 - Cross-pair short to pair A
         
         9 - Cross-pair short to pair B
         
         10 - Cross-pair short to pair C
         
         11 - Cross-pair short to pair D
         
         12 - Abnormal cross-pair coupling - pair A
         
         13 - Abnormal cross-pair coupling - pair B
         
         14 - Abnormal cross-pair coupling - pair C
         
         15 - Abnormal cross-pair coupling - pair D"
    ::= { luxlPortStatusVeriPhyResultEntry 4 }

luxlPortStatusVeriPhyResultVeriPhyStatusPairD OBJECT-TYPE
    SYNTAX      LUXLPortPhyVeriPhyStatus
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "VeriPhy status for the cable pair
         
         0 - Cable is Correctly terminated pair
         
         1 - Open pair
         
         2 - Shorted pair
         
         4 - Abnormal termination
         
         8 - Cross-pair short to pair A
         
         9 - Cross-pair short to pair B
         
         10 - Cross-pair short to pair C
         
         11 - Cross-pair short to pair D
         
         12 - Abnormal cross-pair coupling - pair A
         
         13 - Abnormal cross-pair coupling - pair B
         
         14 - Abnormal cross-pair coupling - pair C
         
         15 - Abnormal cross-pair coupling - pair D"
    ::= { luxlPortStatusVeriPhyResultEntry 5 }

luxlPortStatusVeriPhyResultVeriPhyLengthStatusPairA OBJECT-TYPE
    SYNTAX      LUXLUnsigned8
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "VeriPhy status cable length i meters for the cable pair. When VeriPhy
         is completed, the cable diagnostics results is shown in the VeriPhy
         status table. Note that VeriPHY is only accurate for cables of length 7
         - 140 meters.
         
         The resolution is 3 meters"
    ::= { luxlPortStatusVeriPhyResultEntry 6 }

luxlPortStatusVeriPhyResultVeriPhyLengthStatusPairB OBJECT-TYPE
    SYNTAX      LUXLUnsigned8
    MAX-ACCESS  read-only
