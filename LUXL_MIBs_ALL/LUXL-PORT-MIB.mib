LUXL-PORT-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    luxlSwitchMgmt
        FROM LUXL-SMI
    Counter64 FROM SNMPv2-SMI
    Integer32 FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    LUXLDisplayString FROM LUXL-TC
    LUXLInterfaceIndex FROM LUXL-TC
    LUXLSfpTransceiver FROM LUXL-TC
    LUXLUnsigned8 FROM LUXL-TC
    ;

luxlPortMib MODULE-IDENTITY
    LAST-UPDATED "201407010000Z"
    ORGANIZATION
        "Vitesse Semiconductor Corporation"
    CONTACT-INFO
        "Application support <application_support@vitesse.com>"
    DESCRIPTION
        "This is a private version of the PORT MIB"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { luxlSwitchMgmt 11 }


LUXLPortFc ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration controls the interface flow control."
    SYNTAX      INTEGER { off(0), on(1) }

LUXLPortMedia ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration controls the interface media type."
    SYNTAX      INTEGER { rj45(0), sfp(1), dual(2) }

LUXLPortPhyVeriPhyStatus ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumerations show the VeriPhy status."
    SYNTAX      INTEGER { correctlyTerminatedPair(0), openPair(1), shortPair(2), abnormalTermination(4), crossPairShortToPairA(8), crossPairShortToPairB(9), crossPairShortToPairC(10), crossPairShortToPairD(11), abnormalCrossPairCouplingToPairA(12), abnormalCrossPairCouplingToPairB(13), abnormalCrossPairCouplingToPairC(14), abnormalCrossPairCouplingToPairD(15), unknownResult(16), veriPhyRunning(17) }

LUXLPortSpeed ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration controls the interface speed. E.g force10ModeFdx means
         force 10Mbs full duplex."
    SYNTAX      INTEGER { force10ModeFdx(0), force10ModeHdx(1), force100ModeFdx(2), force100ModeHdx(3), force1GModeFdx(4), autoNegMode(5), force2G5ModeFdx(6), force5GModeFdx(7), force10GModeFdx(8), force12GModeFdx(9) }

LUXLPortStatusSpeed ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumerations show the current interface speed."
    SYNTAX      INTEGER { undefined(0), speed10M(1), speed100M(2), speed1G(3), speed2G5(4), speed5G(5), speed10G(6), speed12G(7) }

luxlPortMibObjects OBJECT IDENTIFIER
    ::= { luxlPortMib 1 }

luxlPortConfig OBJECT IDENTIFIER
    ::= { luxlPortMibObjects 2 }

luxlPortConfigTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LUXLPortConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is a table of the port interface parameters"
    ::= { luxlPortConfig 1 }

luxlPortConfigEntry OBJECT-TYPE
    SYNTAX      LUXLPortConfigEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each port interface has a set of parameters"
    INDEX       { luxlPortConfigIfIndex }
    ::= { luxlPortConfigTable 1 }

LUXLPortConfigEntry ::= SEQUENCE {
    luxlPortConfigIfIndex   LUXLInterfaceIndex,
    luxlPortConfigShutdown      TruthValue,
    luxlPortConfigSpeed     LUXLPortSpeed,
    luxlPortConfigAdvertiseDisabled     LUXLUnsigned8,
    luxlPortConfigMediaType     LUXLPortMedia,
    luxlPortConfigFC    LUXLPortFc,
    luxlPortConfigMTU   Unsigned32,
    luxlPortConfigExcessiveRestart      TruthValue,
    luxlPortConfigPFC   LUXLUnsigned8
}

luxlPortConfigIfIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Logical interface number."
    ::= { luxlPortConfigEntry 1 }

luxlPortConfigShutdown OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Controls whether interface is shutdown or powered up. Set to TRUE in
         order to power down the interface."
    ::= { luxlPortConfigEntry 2 }

luxlPortConfigSpeed OBJECT-TYPE
    SYNTAX      LUXLPortSpeed
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Controls the port speed and duplex."
    ::= { luxlPortConfigEntry 3 }

luxlPortConfigAdvertiseDisabled OBJECT-TYPE
    SYNTAX      LUXLUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "In auto mode, bitmask that allows features not to be advertised.
         
         Bit 0: Disable half duplex advertising.
         
         Bit 1: Disable full duplex advertising.
         
         Bit 4: Disable 1G advertising.
         
         Bit 6: Disable 100M advertising.
         
         Bit 7: Disable 10M advertising.
         
         When not in auto mode, the value shall be zero."
    ::= { luxlPortConfigEntry 4 }

luxlPortConfigMediaType OBJECT-TYPE
    SYNTAX      LUXLPortMedia
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Controls the port media type."
    ::= { luxlPortConfigEntry 5 }

luxlPortConfigFC OBJECT-TYPE
    SYNTAX      LUXLPortFc
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Controls the port flow control mode."
    ::= { luxlPortConfigEntry 6 }

luxlPortConfigMTU OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Controls the port's Maximum Transmission Unit."
    ::= { luxlPortConfigEntry 7 }

luxlPortConfigExcessiveRestart OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "TRUE to restart half-duplex back-off algorithm after 16 collisions.
         FALSE to discard frame after 16 collisions"
    ::= { luxlPortConfigEntry 8 }

luxlPortConfigPFC OBJECT-TYPE
    SYNTAX      LUXLUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "802.1Qbb Priority Flow Control bitmask, one bit for each prioritet.E.g.
         0x01 = prio 0, 0x80 = prio 7, 0xFF = prio 0-7"
    ::= { luxlPortConfigEntry 9 }

luxlPortStatus OBJECT IDENTIFIER
