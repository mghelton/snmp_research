    SYNTAX      SEQUENCE OF LUXLIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "IPv4 interface configuration table. This table enables IPv4 related
         configuration of the corresponding IP interface."
    ::= { luxlIpConfigInterfaces 3 }

luxlIpConfigInterfacesIpv4Entry OBJECT-TYPE
    SYNTAX      LUXLIpConfigInterfacesIpv4Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an IP interface."
    INDEX       { luxlIpConfigInterfacesIpv4IfIndex }
    ::= { luxlIpConfigInterfacesIpv4Table 1 }

LUXLIpConfigInterfacesIpv4Entry ::= SEQUENCE {
    luxlIpConfigInterfacesIpv4IfIndex   LUXLInterfaceIndex,
    luxlIpConfigInterfacesIpv4Active    TruthValue,
    luxlIpConfigInterfacesIpv4EnableDhcpClient      TruthValue,
    luxlIpConfigInterfacesIpv4Ipv4Address   IpAddress,
    luxlIpConfigInterfacesIpv4PrefixSize    Unsigned32,
    luxlIpConfigInterfacesIpv4DhcpClientFallbackTimeout     Unsigned32
}

luxlIpConfigInterfacesIpv4IfIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { luxlIpConfigInterfacesIpv4Entry 1 }

luxlIpConfigInterfacesIpv4Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable IPv4.
         
         IPv4 can only be enabled if either the DHCP client is enabled, or a
         valid address has been configured."
    ::= { luxlIpConfigInterfacesIpv4Entry 2 }

luxlIpConfigInterfacesIpv4EnableDhcpClient OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable IPv4 DHCP client.
         
         Note: the DHCP client can only be enabled if there is no conflict in
         the values of: ipv4Address, prefixSize, and dhcpClientFallbackTimeout."
    ::= { luxlIpConfigInterfacesIpv4Entry 3 }

luxlIpConfigInterfacesIpv4Ipv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "IPv4 address.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { luxlIpConfigInterfacesIpv4Entry 4 }

luxlIpConfigInterfacesIpv4PrefixSize OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Prefix size of the network.
         
         Note: Multiple interfaces may not have overlapping networks."
    ::= { luxlIpConfigInterfacesIpv4Entry 5 }

luxlIpConfigInterfacesIpv4DhcpClientFallbackTimeout OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "DHCP client fallback timer.
         
         If DHCP is disabled then this object has no effect. If DHCP is enabled
         and the fallback timeout value is different from zero, then this timer
         will stop the DHCP process and assign the ipv4Address to the interface
         instead."
    ::= { luxlIpConfigInterfacesIpv4Entry 6 }

luxlIpConfigInterfacesIpv6Table OBJECT-TYPE
    SYNTAX      SEQUENCE OF LUXLIpConfigInterfacesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "IPv6 interface configuration table. This table enables IPv6 related
         configuration of the corresponding IP interface."
    ::= { luxlIpConfigInterfaces 4 }

luxlIpConfigInterfacesIpv6Entry OBJECT-TYPE
    SYNTAX      LUXLIpConfigInterfacesIpv6Entry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry in this table represents an IP interface."
    INDEX       { luxlIpConfigInterfacesIpv6IfIndex }
    ::= { luxlIpConfigInterfacesIpv6Table 1 }

LUXLIpConfigInterfacesIpv6Entry ::= SEQUENCE {
    luxlIpConfigInterfacesIpv6IfIndex   LUXLInterfaceIndex,
    luxlIpConfigInterfacesIpv6Active    TruthValue,
    luxlIpConfigInterfacesIpv6Ipv6Address   InetAddressIPv6,
    luxlIpConfigInterfacesIpv6PrefixSize    Unsigned32
}

luxlIpConfigInterfacesIpv6IfIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { luxlIpConfigInterfacesIpv6Entry 1 }

luxlIpConfigInterfacesIpv6Active OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable the static configured IPv6 address.
         
         The static configured IPv6 address can only be configured if a valid
         address has been written into 'ipv6Address' and 'prefixSize'."
    ::= { luxlIpConfigInterfacesIpv6Entry 2 }

luxlIpConfigInterfacesIpv6Ipv6Address OBJECT-TYPE
    SYNTAX      InetAddressIPv6
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Static configured IPv6 address."
