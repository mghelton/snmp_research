    ::= { luxlIpStatusGlobalsIpv6NeighbourTable 1 }

LUXLIpStatusGlobalsIpv6NeighbourEntry ::= SEQUENCE {
    luxlIpStatusGlobalsIpv6NeighbourIpAddress   InetAddressIPv6,
