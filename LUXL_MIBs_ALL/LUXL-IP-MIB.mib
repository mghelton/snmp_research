    luxlIpStatisticsInterfacesIpv6InForwDatagrams   Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCInForwDatagrams     Counter64,
    luxlIpStatisticsInterfacesIpv6ReasmReqds    Unsigned32,
    luxlIpStatisticsInterfacesIpv6ReasmOKs      Unsigned32,
    luxlIpStatisticsInterfacesIpv6ReasmFails    Unsigned32,
    luxlIpStatisticsInterfacesIpv6InDiscards    Unsigned32,
    luxlIpStatisticsInterfacesIpv6InDelivers    Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCInDelivers      Counter64,
    luxlIpStatisticsInterfacesIpv6OutRequests   Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCOutRequests     Counter64,
    luxlIpStatisticsInterfacesIpv6OutNoRoutes   Unsigned32,
    luxlIpStatisticsInterfacesIpv6OutForwDatagrams      Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCOutForwDatagrams    Counter64,
    luxlIpStatisticsInterfacesIpv6OutDiscards   Unsigned32,
    luxlIpStatisticsInterfacesIpv6OutFragReqds      Unsigned32,
    luxlIpStatisticsInterfacesIpv6OutFragOKs    Unsigned32,
    luxlIpStatisticsInterfacesIpv6OutFragFails      Unsigned32,
    luxlIpStatisticsInterfacesIpv6OutFragCreates    Unsigned32,
    luxlIpStatisticsInterfacesIpv6OutTransmits      Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCOutTransmits    Counter64,
    luxlIpStatisticsInterfacesIpv6OutOctets     Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCOutOctets   Counter64,
    luxlIpStatisticsInterfacesIpv6InMcastPkts   Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCInMcastPkts     Counter64,
    luxlIpStatisticsInterfacesIpv6InMcastOctets     Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCInMcastOctets   Counter64,
    luxlIpStatisticsInterfacesIpv6OutMcastPkts      Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCOutMcastPkts    Counter64,
    luxlIpStatisticsInterfacesIpv6OutMcastOctets    Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCOutMcastOctets      Counter64,
    luxlIpStatisticsInterfacesIpv6InBcastPkts   Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCInBcastPkts     Counter64,
    luxlIpStatisticsInterfacesIpv6OutBcastPkts      Unsigned32,
    luxlIpStatisticsInterfacesIpv6HCOutBcastPkts    Counter64,
    luxlIpStatisticsInterfacesIpv6DiscontinuityTime     Counter64,
    luxlIpStatisticsInterfacesIpv6RefreshRate   Unsigned32
}

luxlIpStatisticsInterfacesIpv6IfIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Interface index number."
    ::= { luxlIpStatisticsInterfacesIpv6Entry 1 }

luxlIpStatisticsInterfacesIpv6InReceives OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Number of input IP datagrams received, including those received in
         error.
         
         Discontinuities in the value of this counter may occur at
         re-initialization of the system, and at other times as indicated by the
         value of DiscontinuityTime."
    ::= { luxlIpStatisticsInterfacesIpv6Entry 2 }

luxlIpStatisticsInterfacesIpv6HCInReceives OBJECT-TYPE
    SYNTAX      Counter64
    MAX-ACCESS  read-only
    STATUS      current
    DESCRIPTION 
        "Number of input IP datagrams received, including those received in
         error. This object counts the same datagrams as InReceives, but allows
         for larger values.
         
         Discontinuities in the value of this counter may occur at
         re-initialization of the system, and at other times as indicated by the
         value of DiscontinuityTime."
