    luxlIpStatisticsInterfacesIpv4ReasmOKs      Unsigned32,
    luxlIpStatisticsInterfacesIpv4ReasmFails    Unsigned32,
    luxlIpStatisticsInterfacesIpv4InDiscards    Unsigned32,
    luxlIpStatisticsInterfacesIpv4InDelivers    Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCInDelivers      Counter64,
    luxlIpStatisticsInterfacesIpv4OutRequests   Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCOutRequests     Counter64,
    luxlIpStatisticsInterfacesIpv4OutNoRoutes   Unsigned32,
    luxlIpStatisticsInterfacesIpv4OutForwDatagrams      Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCOutForwDatagrams    Counter64,
    luxlIpStatisticsInterfacesIpv4OutDiscards   Unsigned32,
    luxlIpStatisticsInterfacesIpv4OutFragReqds      Unsigned32,
    luxlIpStatisticsInterfacesIpv4OutFragOKs    Unsigned32,
    luxlIpStatisticsInterfacesIpv4OutFragFails      Unsigned32,
    luxlIpStatisticsInterfacesIpv4OutFragCreates    Unsigned32,
    luxlIpStatisticsInterfacesIpv4OutTransmits      Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCOutTransmits    Counter64,
    luxlIpStatisticsInterfacesIpv4OutOctets     Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCOutOctets   Counter64,
    luxlIpStatisticsInterfacesIpv4InMcastPkts   Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCInMcastPkts     Counter64,
    luxlIpStatisticsInterfacesIpv4InMcastOctets     Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCInMcastOctets   Counter64,
    luxlIpStatisticsInterfacesIpv4OutMcastPkts      Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCOutMcastPkts    Counter64,
    luxlIpStatisticsInterfacesIpv4OutMcastOctets    Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCOutMcastOctets      Counter64,
    luxlIpStatisticsInterfacesIpv4InBcastPkts   Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCInBcastPkts     Counter64,
    luxlIpStatisticsInterfacesIpv4OutBcastPkts      Unsigned32,
    luxlIpStatisticsInterfacesIpv4HCOutBcastPkts    Counter64,
    luxlIpStatisticsInterfacesIpv4DiscontinuityTime     Counter64,
    luxlIpStatisticsInterfacesIpv4RefreshRate   Unsigned32
