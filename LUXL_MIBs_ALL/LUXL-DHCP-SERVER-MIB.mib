LUXL-DHCP-SERVER-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    luxlSwitchMgmt
        FROM LUXL-SMI
    Integer32 FROM SNMPv2-SMI
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    MacAddress FROM SNMPv2-TC
    TruthValue FROM SNMPv2-TC
    LUXLDisplayString FROM LUXL-TC
    LUXLInterfaceIndex FROM LUXL-TC
    LUXLRowEditorState FROM LUXL-TC
    LUXLUnsigned16 FROM LUXL-TC
    ;

luxlDhcpServerMib MODULE-IDENTITY
    LAST-UPDATED "201411270000Z"
    ORGANIZATION
        "Vitesse Semiconductor Corporation"
    CONTACT-INFO
        "Application support <application_support@vitesse.com>"
    DESCRIPTION
        "This is a private version of DhcpServer"
    REVISION    "201411270000Z"
    DESCRIPTION
        "revise descriptions by Palle"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { luxlSwitchMgmt 109 }


LUXLDhcpServerBindingEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of binding."
    SYNTAX      INTEGER { none(0), automatic(1), manual(2), expired(3) }

LUXLDhcpServerBindingStateEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the state of binding."
    SYNTAX      INTEGER { none(0), allocated(1), committed(2), expired(3) }

LUXLDhcpServerClientIdentifierEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of client identifier."
    SYNTAX      INTEGER { none(0), fqdn(1), mac(2) }

LUXLDhcpServerNetbiosNodeEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of NetBIOS node."
    SYNTAX      INTEGER { nodeNone(0), nodeB(1), nodeP(2), nodeM(3), nodeH(4) }

LUXLDhcpServerPoolEnum ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration defines the type of DHCP pool."
    SYNTAX      INTEGER { none(0), network(1), host(2) }

luxlDhcpServerMibObjects OBJECT IDENTIFIER
    ::= { luxlDhcpServerMib 1 }

luxlDhcpServerConfig OBJECT IDENTIFIER
    ::= { luxlDhcpServerMibObjects 2 }

luxlDhcpServerConfigGlobals OBJECT IDENTIFIER
    ::= { luxlDhcpServerConfig 1 }

luxlDhcpServerConfigGlobalsMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Global mode of DHCP server. true is to enable the functions of DHCP
         server and false is to disable it."
    ::= { luxlDhcpServerConfigGlobals 1 }

luxlDhcpServerConfigVlanTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LUXLDhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is the table of DHCP server VLAN configuration. The index is VLAN
         ID."
    ::= { luxlDhcpServerConfig 2 }

luxlDhcpServerConfigVlanEntry OBJECT-TYPE
    SYNTAX      LUXLDhcpServerConfigVlanEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each VLAN has a set of parameters"
    INDEX       { luxlDhcpServerConfigVlanIfIndex }
    ::= { luxlDhcpServerConfigVlanTable 1 }

LUXLDhcpServerConfigVlanEntry ::= SEQUENCE {
    luxlDhcpServerConfigVlanIfIndex     LUXLInterfaceIndex,
    luxlDhcpServerConfigVlanMode    TruthValue
}

luxlDhcpServerConfigVlanIfIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Logical interface number of VLAN."
    ::= { luxlDhcpServerConfigVlanEntry 1 }

luxlDhcpServerConfigVlanMode OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "VLAN mode of DHCP server. true is to enable DHCP server per VLAN and
         false is to disable it per VLAN."
    ::= { luxlDhcpServerConfigVlanEntry 2 }

luxlDhcpServerConfigExcludedTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LUXLDhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The table is DHCP server excluded IP onfiguration table. The indexes
         are low IP and high IP address."
    ::= { luxlDhcpServerConfig 3 }

luxlDhcpServerConfigExcludedEntry OBJECT-TYPE
    SYNTAX      LUXLDhcpServerConfigExcludedEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry has a set of parameters"
    INDEX       { luxlDhcpServerConfigExcludedLowIpAddress, luxlDhcpServerConfigExcludedHighIpAddress }
    ::= { luxlDhcpServerConfigExcludedTable 1 }

LUXLDhcpServerConfigExcludedEntry ::= SEQUENCE {
    luxlDhcpServerConfigExcludedLowIpAddress    IpAddress,
    luxlDhcpServerConfigExcludedHighIpAddress   IpAddress,
    luxlDhcpServerConfigExcludedAction      LUXLRowEditorState
}

luxlDhcpServerConfigExcludedLowIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Low IP address."
    ::= { luxlDhcpServerConfigExcludedEntry 1 }

luxlDhcpServerConfigExcludedHighIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "High IP address."
    ::= { luxlDhcpServerConfigExcludedEntry 2 }

luxlDhcpServerConfigExcludedAction OBJECT-TYPE
    SYNTAX      LUXLRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Action"
    ::= { luxlDhcpServerConfigExcludedEntry 100 }

luxlDhcpServerConfigExcludedIpTableRowEditor OBJECT IDENTIFIER
    ::= { luxlDhcpServerConfig 4 }

luxlDhcpServerConfigExcludedIpTableRowEditorLowIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Low IP address."
    ::= { luxlDhcpServerConfigExcludedIpTableRowEditor 1 }

luxlDhcpServerConfigExcludedIpTableRowEditorHighIpAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "High IP address."
    ::= { luxlDhcpServerConfigExcludedIpTableRowEditor 2 }

luxlDhcpServerConfigExcludedIpTableRowEditorAction OBJECT-TYPE
    SYNTAX      LUXLRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Action"
    ::= { luxlDhcpServerConfigExcludedIpTableRowEditor 100 }

luxlDhcpServerConfigPoolTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LUXLDhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "The table is DHCP server pool onfiguration table. The indexe is pool
         name."
    ::= { luxlDhcpServerConfig 5 }

luxlDhcpServerConfigPoolEntry OBJECT-TYPE
    SYNTAX      LUXLDhcpServerConfigPoolEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry has a set of parameters"
    INDEX       { luxlDhcpServerConfigPoolPoolName }
    ::= { luxlDhcpServerConfigPoolTable 1 }

LUXLDhcpServerConfigPoolEntry ::= SEQUENCE {
    luxlDhcpServerConfigPoolPoolName    LUXLDisplayString,
    luxlDhcpServerConfigPoolPoolType    LUXLDhcpServerPoolEnum,
    luxlDhcpServerConfigPoolIpv4Address     IpAddress,
    luxlDhcpServerConfigPoolSubnetMask      IpAddress,
    luxlDhcpServerConfigPoolSubnetBroadcast     IpAddress,
    luxlDhcpServerConfigPoolLeaseDay    Unsigned32,
    luxlDhcpServerConfigPoolLeaseHour   Unsigned32,
    luxlDhcpServerConfigPoolLeaseMinute     Unsigned32,
    luxlDhcpServerConfigPoolDomainName      LUXLDisplayString,
    luxlDhcpServerConfigPoolDefaultRouter1      IpAddress,
    luxlDhcpServerConfigPoolDefaultRouter2      IpAddress,
    luxlDhcpServerConfigPoolDefaultRouter3      IpAddress,
    luxlDhcpServerConfigPoolDefaultRouter4      IpAddress,
    luxlDhcpServerConfigPoolDnsServer1      IpAddress,
    luxlDhcpServerConfigPoolDnsServer2      IpAddress,
    luxlDhcpServerConfigPoolDnsServer3      IpAddress,
    luxlDhcpServerConfigPoolDnsServer4      IpAddress,
    luxlDhcpServerConfigPoolNtpServer1      IpAddress,
    luxlDhcpServerConfigPoolNtpServer2      IpAddress,
    luxlDhcpServerConfigPoolNtpServer3      IpAddress,
    luxlDhcpServerConfigPoolNtpServer4      IpAddress,
    luxlDhcpServerConfigPoolNetbiosNodeType     LUXLDhcpServerNetbiosNodeEnum,
    luxlDhcpServerConfigPoolNetbiosScope    LUXLDisplayString,
    luxlDhcpServerConfigPoolNetbiosNameServer1      IpAddress,
    luxlDhcpServerConfigPoolNetbiosNameServer2      IpAddress,
    luxlDhcpServerConfigPoolNetbiosNameServer3      IpAddress,
    luxlDhcpServerConfigPoolNetbiosNameServer4      IpAddress,
    luxlDhcpServerConfigPoolNisDomainName   LUXLDisplayString,
    luxlDhcpServerConfigPoolNisServer1      IpAddress,
    luxlDhcpServerConfigPoolNisServer2      IpAddress,
    luxlDhcpServerConfigPoolNisServer3      IpAddress,
    luxlDhcpServerConfigPoolNisServer4      IpAddress,
    luxlDhcpServerConfigPoolClientIdentifierType    LUXLDhcpServerClientIdentifierEnum,
    luxlDhcpServerConfigPoolClientIdentifierFqdn    LUXLDisplayString,
    luxlDhcpServerConfigPoolClientIdentifierMac     MacAddress,
    luxlDhcpServerConfigPoolClientHardwareAddress   MacAddress,
    luxlDhcpServerConfigPoolClientName      LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorClassId1      LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorSpecificInfo1     LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorClassId2      LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorSpecificInfo2     LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorClassId3      LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorSpecificInfo3     LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorClassId4      LUXLDisplayString,
    luxlDhcpServerConfigPoolVendorSpecificInfo4     LUXLDisplayString,
    luxlDhcpServerConfigPoolAction      LUXLRowEditorState
}

luxlDhcpServerConfigPoolPoolName OBJECT-TYPE
    SYNTAX      LUXLDisplayString (SIZE(0..32))
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Name of DHCP pool."
    ::= { luxlDhcpServerConfigPoolEntry 1 }

luxlDhcpServerConfigPoolPoolType OBJECT-TYPE
    SYNTAX      LUXLDhcpServerPoolEnum
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Type of pool. none(0) means the pool type is not defined yet.
         network(1) means the pool defines a pool of IP addresses to service
         more than one DHCP client. host(2) means the pool services for a
         specific DHCP client identified by client identifier or hardware
         address."
    ::= { luxlDhcpServerConfigPoolEntry 2 }

luxlDhcpServerConfigPoolIpv4Address OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Network number of the subnet. If the pool type is of network, the IP
         address can be any general IP address. If the pool type is of host, the
         IP address must be a unicast IP address."
    ::= { luxlDhcpServerConfigPoolEntry 3 }

luxlDhcpServerConfigPoolSubnetMask OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Subnet Mask. DHCP option 1. Specify subnet mask of the DHCP address
         pool, excluding 0.0.0.0 and 255.255.255.255."
    ::= { luxlDhcpServerConfigPoolEntry 4 }

luxlDhcpServerConfigPoolSubnetBroadcast OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Broadcast IP address in the subnet. DHCP option 28. Specify the
         broadcast address in use on the client's subnet."
    ::= { luxlDhcpServerConfigPoolEntry 5 }

luxlDhcpServerConfigPoolLeaseDay OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Number of days of lease time. DHCP option 51, 58 and 59. The value
         range is 0-365. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { luxlDhcpServerConfigPoolEntry 6 }

luxlDhcpServerConfigPoolLeaseHour OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Number of hours of lease time. DHCP option 51, 58 and 59. The value
         range is 0-23. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { luxlDhcpServerConfigPoolEntry 7 }

luxlDhcpServerConfigPoolLeaseMinute OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Number of minutes of lease time. DHCP option 51, 58 and 59. The value
         range is 0-59. Specify lease time that allows the client to request a
         lease time for the IP address. If all of LeaseDay, LeaseHour and
         LeaseMinute are 0's, then it means the lease time is infinite."
    ::= { luxlDhcpServerConfigPoolEntry 8 }

luxlDhcpServerConfigPoolDomainName OBJECT-TYPE
    SYNTAX      LUXLDisplayString (SIZE(0..32))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Domain name. DHCP option 15. Specify domain name that client should use
         when resolving hostname via DNS."
    ::= { luxlDhcpServerConfigPoolEntry 9 }

luxlDhcpServerConfigPoolDefaultRouter1 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Default router 1."
    ::= { luxlDhcpServerConfigPoolEntry 10 }

luxlDhcpServerConfigPoolDefaultRouter2 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Default router 2."
    ::= { luxlDhcpServerConfigPoolEntry 11 }

luxlDhcpServerConfigPoolDefaultRouter3 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
