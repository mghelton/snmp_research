    DESCRIPTION 
        "Default router 3."
    ::= { luxlDhcpServerConfigPoolEntry 12 }

luxlDhcpServerConfigPoolDefaultRouter4 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Default router 4."
    ::= { luxlDhcpServerConfigPoolEntry 13 }

luxlDhcpServerConfigPoolDnsServer1 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "DNS server 1."
    ::= { luxlDhcpServerConfigPoolEntry 14 }

luxlDhcpServerConfigPoolDnsServer2 OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "DNS server 2."
    ::= { luxlDhcpServerConfigPoolEntry 15 }

luxlDhcpServerConfigPoolDnsServer3 OBJECT-TYPE
    SYNTAX      IpAddress
