LUXL-IPMC-MVR-MIB DEFINITIONS ::= BEGIN

IMPORTS
    MODULE-COMPLIANCE, OBJECT-GROUP
        FROM SNMPv2-CONF
    MODULE-IDENTITY, OBJECT-TYPE
        FROM SNMPv2-SMI
    TEXTUAL-CONVENTION
        FROM SNMPv2-TC
    luxlSwitchMgmt
        FROM LUXL-SMI
    InetAddressIPv6 FROM INET-ADDRESS-MIB
    IpAddress FROM SNMPv2-SMI
    Unsigned32 FROM SNMPv2-SMI
    TruthValue FROM SNMPv2-TC
    LUXLDisplayString FROM LUXL-TC
    LUXLInterfaceIndex FROM LUXL-TC
    LUXLPortList FROM LUXL-TC
    LUXLRowEditorState FROM LUXL-TC
    LUXLUnsigned8 FROM LUXL-TC
    ;

luxlIpmcMvrMib MODULE-IDENTITY
    LAST-UPDATED "201407010000Z"
    ORGANIZATION
        "Vitesse Semiconductor Corporation"
    CONTACT-INFO
        "Application support <application_support@vitesse.com>"
    DESCRIPTION
        "This is a private version of the IPMC MVR MIB"
    REVISION    "201407010000Z"
    DESCRIPTION
        "Initial version"
    ::= { luxlSwitchMgmt 68 }


LUXLIpmcMvrGroupSrcListGroupFilterMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates the group filter mode for an IPMC group
         address."
    SYNTAX      INTEGER { exclude(0), include(1) }

LUXLIpmcMvrGroupSrcListSourceType ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates the source filtering type from IPMC."
    SYNTAX      INTEGER { deny(0), permit(1) }

LUXLIpmcMvrInterfaceQuerierStatus ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates the Querier status for MVR VLAN interface."
    SYNTAX      INTEGER { disabled(-1), initial(0), idle(1), active(2) }

LUXLIpmcMvrVlanInterfaceMode ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates the MVR VLAN interface's operational mode."
    SYNTAX      INTEGER { dynamic(0), compatible(1) }

LUXLIpmcMvrVlanInterfacePortRole ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates the MVR port's operational role."
    SYNTAX      INTEGER { inactive(0), source(1), receiver(2) }

LUXLIpmcMvrVlanInterfaceTagging ::= TEXTUAL-CONVENTION
    STATUS      current
    DESCRIPTION
        "This enumeration indicates the VLAN tagging for IPMC control frames in
         MVR."
    SYNTAX      INTEGER { untagged(0), tagging(1) }

luxlIpmcMvrMibObjects OBJECT IDENTIFIER
    ::= { luxlIpmcMvrMib 1 }

luxlIpmcMvrConfig OBJECT IDENTIFIER
    ::= { luxlIpmcMvrMibObjects 2 }

luxlIpmcMvrConfigGlobals OBJECT IDENTIFIER
    ::= { luxlIpmcMvrConfig 1 }

luxlIpmcMvrConfigGlobalsAdminState OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable/Disable the IPMC MVR global functionality."
    ::= { luxlIpmcMvrConfigGlobals 1 }

luxlIpmcMvrConfigPortTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LUXLIpmcMvrConfigPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is a table for managing extra MVR helper features per port basis."
    ::= { luxlIpmcMvrConfig 2 }

luxlIpmcMvrConfigPortEntry OBJECT-TYPE
    SYNTAX      LUXLIpmcMvrConfigPortEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each port has a set of parameters."
    INDEX       { luxlIpmcMvrConfigPortPortIndex }
    ::= { luxlIpmcMvrConfigPortTable 1 }

LUXLIpmcMvrConfigPortEntry ::= SEQUENCE {
    luxlIpmcMvrConfigPortPortIndex      LUXLInterfaceIndex,
    luxlIpmcMvrConfigPortDoImmediateLeave   TruthValue
}

luxlIpmcMvrConfigPortPortIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Logical interface number of the physical port."
    ::= { luxlIpmcMvrConfigPortEntry 1 }

luxlIpmcMvrConfigPortDoImmediateLeave OBJECT-TYPE
    SYNTAX      TruthValue
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Enable/Disable the MVR immediate leave functionality."
    ::= { luxlIpmcMvrConfigPortEntry 2 }

luxlIpmcMvrConfigInterfaceTable OBJECT-TYPE
    SYNTAX      SEQUENCE OF LUXLIpmcMvrConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "This is a table for managing MVR VLAN interface entries."
    ::= { luxlIpmcMvrConfig 3 }

luxlIpmcMvrConfigInterfaceEntry OBJECT-TYPE
    SYNTAX      LUXLIpmcMvrConfigInterfaceEntry
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Each entry has a set of parameters"
    INDEX       { luxlIpmcMvrConfigInterfaceIfIndex }
    ::= { luxlIpmcMvrConfigInterfaceTable 1 }

LUXLIpmcMvrConfigInterfaceEntry ::= SEQUENCE {
    luxlIpmcMvrConfigInterfaceIfIndex   LUXLInterfaceIndex,
    luxlIpmcMvrConfigInterfaceName      LUXLDisplayString,
    luxlIpmcMvrConfigInterfaceIgmpQuerierAddress    IpAddress,
    luxlIpmcMvrConfigInterfaceMode      LUXLIpmcMvrVlanInterfaceMode,
    luxlIpmcMvrConfigInterfaceTagging   LUXLIpmcMvrVlanInterfaceTagging,
    luxlIpmcMvrConfigInterfacePriority      LUXLUnsigned8,
    luxlIpmcMvrConfigInterfaceLastListenerQueryInt      Unsigned32,
    luxlIpmcMvrConfigInterfaceChannelProfile    LUXLDisplayString,
    luxlIpmcMvrConfigInterfaceAction    LUXLRowEditorState
}

luxlIpmcMvrConfigInterfaceIfIndex OBJECT-TYPE
    SYNTAX      LUXLInterfaceIndex
    MAX-ACCESS  not-accessible
    STATUS      current
    DESCRIPTION 
        "Logical interface number of the VLAN interface."
    ::= { luxlIpmcMvrConfigInterfaceEntry 1 }

luxlIpmcMvrConfigInterfaceName OBJECT-TYPE
    SYNTAX      LUXLDisplayString (SIZE(0..16))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "MVR Name is an optional attribute to indicate the name of the specific
         MVR VLAN that user could easily associate the MVR VLAN purpose with its
         name."
    ::= { luxlIpmcMvrConfigInterfaceEntry 2 }

luxlIpmcMvrConfigInterfaceIgmpQuerierAddress OBJECT-TYPE
    SYNTAX      IpAddress
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The static IPv4 source address of the specific MVR interface for seding
         IGMP Query message with respect to IGMP Querier election."
    ::= { luxlIpmcMvrConfigInterfaceEntry 3 }

luxlIpmcMvrConfigInterfaceMode OBJECT-TYPE
    SYNTAX      LUXLIpmcMvrVlanInterfaceMode
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "In Dynamic mode, MVR allows dynamic MVR membership reports on source
         ports. In Compatible mode, MVR membership reports are forbidden on
         source ports."
    ::= { luxlIpmcMvrConfigInterfaceEntry 4 }

luxlIpmcMvrConfigInterfaceTagging OBJECT-TYPE
    SYNTAX      LUXLIpmcMvrVlanInterfaceTagging
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Specify whether the traversed IGMP/MLD control frames will be sent as
         Untagged or Tagged with MVR VID."
    ::= { luxlIpmcMvrConfigInterfaceEntry 5 }

luxlIpmcMvrConfigInterfacePriority OBJECT-TYPE
    SYNTAX      LUXLUnsigned8
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Specify how the traversed IGMP/MLD control frames will be sent in
         prioritized manner in VLAN tag."
    ::= { luxlIpmcMvrConfigInterfaceEntry 6 }

luxlIpmcMvrConfigInterfaceLastListenerQueryInt OBJECT-TYPE
    SYNTAX      Unsigned32
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "This setting Last Listener/Member Query Interval is used to control
         IGMP protocol stack for fast aging mechanism. It defines the maximum
         time to wait for IGMP/MLD report memberships on a port before removing
         the port from multicast group membership. The value is in units of
         tenths of a seconds. The range is from 0 to 31744."
    ::= { luxlIpmcMvrConfigInterfaceEntry 7 }

luxlIpmcMvrConfigInterfaceChannelProfile OBJECT-TYPE
    SYNTAX      LUXLDisplayString (SIZE(0..16))
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "The profile used for the channel filtering condition in the specific
         MVR VLAN. Profile selected for designated interface channel is not
         allowed to have overlapped permit group address by comparing with other
         MVR VLAN interface's channel."
    ::= { luxlIpmcMvrConfigInterfaceEntry 8 }

luxlIpmcMvrConfigInterfaceAction OBJECT-TYPE
    SYNTAX      LUXLRowEditorState
    MAX-ACCESS  read-write
    STATUS      current
    DESCRIPTION 
        "Action"
    ::= { luxlIpmcMvrConfigInterfaceEntry 100 }

luxlIpmcMvrConfigInterfaceTableRowEditor OBJECT IDENTIFIER
    ::= { luxlIpmcMvrConfig 4 }

luxlIpmcMvrConfigInterfaceTableRowEditorIfIndex OBJECT-TYPE
