    luxlIpmcMvrStatusIgmpVlanCounterRxV2Leave   Unsigned32,
    luxlIpmcMvrStatusIgmpVlanCounterRxV3Join    Unsigned32,
    luxlIpmcMvrStatusIgmpVlanCounterRxErrors    Unsigned32
}

luxlIpmcMvrStatusIgmpVlanIfIndex OBJECT-TYPE
